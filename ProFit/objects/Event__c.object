    <validationRules>
        <fullName>End_Date_must_1_day_ahead_of_Start_Date</fullName>
        <active>true</active>
        <description>End Date/Time must be at-least 1 day ahead of Start Date/Time ( If there is a value in End Date/Time field )</description>
        <errorConditionFormula>AND(DATEVALUE( End_Date_Time__c )&lt;(DATEVALUE( Start_Date_Time__c )+1), NOT( ISNULL( End_Date_Time__c ) )   )</errorConditionFormula>
        <errorDisplayField>End_Date_Time__c</errorDisplayField>
        <errorMessage>End Date/Time must be at-least 1 day ahead of Start Date/Time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Event_Type_In_Person</fullName>
        <active>true</active>
        <description>If Event Type field value is In-Person then user must need to select Location on Event Record.</description>
        <errorConditionFormula>AND( ISPICKVAL(  Event_Type__c  , &apos;In-person&apos;) ,   (ISBLANK( Event_Location__c  ) ) )</errorConditionFormula>
        <errorDisplayField>Event_Location__c</errorDisplayField>
        <errorMessage>If Event Type field value is In-Person then user must need to select Location on Event Record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Event_Type_Virtual</fullName>
        <active>true</active>
        <description>If Virtual is Selected as Value for Event Type field then Prevent User to Select Location on Event Record.</description>
        <errorConditionFormula>AND( ISPICKVAL(  Event_Type__c  , &apos;Virtual&apos;) ,   NOT(ISBLANK( Event_Location__c  ) ) )</errorConditionFormula>
        <errorDisplayField>Event_Location__c</errorDisplayField>
        <errorMessage>If Virtual is Selected as Value for Event Type field then Prevent User to Select Location on Event Record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Recurring_checked</fullName>
        <active>true</active>
        <description>If Recurring? checkbox is checked then user must need to fill Frequency field</description>
        <errorConditionFormula>AND( ISBLANK( TEXT( Frequency__c ) ), Recurring__c)</errorConditionFormula>
        <errorDisplayField>Frequency__c</errorDisplayField>
        <errorMessage>Recurring? checkbox is checked then user must need to fill Frequency field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Recurring_unchecked</fullName>
        <active>true</active>
        <description>Recurring? checkbox is unchecked then User can not select Frequency field</description>
        <errorConditionFormula>AND(  NOT(ISBLANK( TEXT( Frequency__c ))  ),  NOT(Recurring__c) )</errorConditionFormula>
        <errorDisplayField>Frequency__c</errorDisplayField>
        <errorMessage>Recurring? checkbox is unchecked then User can not select Frequency field</errorMessage>
    </validationRules>
